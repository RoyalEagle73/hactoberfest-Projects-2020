#include <stdio.h>

int main(){
	int t;
	scanf("%d", &t);
	for(int i=0;i<t;i++)
	{
		int n,k=0;
		scanf("%d",&n);
		while(k<=n||n!=1)
		{
			if(n%2==0)
			{
				n=n/2;
			}
			else
				n=3*n+1;
			k++;
		}
		if(n==1)
		{
			printf("YES\n");

		}
		else
		printf("NO\n");
	}              			
return 0;
}
