#include <stdio.h>
#include <string.h>
int main()
{
    char s[100];
    int i, length,f=0;
    scanf("%s", s);
    
    length = strlen(s);
    
    for(i=0;i < length ;i++)
    {
        if(s[i] != s[length-i-1]){
            f=1;
            break;
   }
}
    
    if (f)
    {
        printf("NO");
    }    
    else 
    {
        printf("YES");
    }
    return 0;
}
