#include <stdio.h>

int main() {
int i,j;
for(i=1;i<=3;i++)
{
    for(j=1;j<=15;j++)
    {
        printf("#");
    }
    printf("\n");
}
for(i=1;i<=11;i++)
{
    printf("#\n");
}
for(i=1;i<=5;i++)
{
    for(j=1;j<=i;j++)
    {
        printf("*");
    }
    printf("\n");
}
printf("\t I LOVE MY  INDIA");
printf("\t \n  INDIA ...INDIA\n");
printf("Proud to be an Indian");
    return 0;
}
